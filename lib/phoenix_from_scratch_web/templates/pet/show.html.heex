<h1>Show Pet</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @pet.name %>
  </li>

  <li>
    <strong>Species:</strong>
    <%= @pet.species %>
  </li>

  <li>
    <strong>Breed:</strong>
    <%= @pet.breed %>
  </li>

  <li>
    <strong>Dob:</strong>
    <%= @pet.dob %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.pet_path(@conn, :edit, @pet) %></span> |
<span><%= link "Back", to: Routes.pet_path(@conn, :index) %></span>
